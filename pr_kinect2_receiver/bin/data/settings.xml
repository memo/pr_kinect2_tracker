        <z>0.0</z>
      </pos>
      <rot>
        <x>0.0</x>
        <y>0.0</y>
        <z>0.0</z>
      </rot>
    </receiver>
  </Receivers>

  <Sender>
    <port>8000</port>
    <ipAddress>127.0.0.1</ipAddress>
  </Sender>
</settings>