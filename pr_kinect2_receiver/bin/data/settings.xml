        <z>0.0</z>
      </pos>
      <rot>
        <x>0.0</x>
        <y>0.0</y>
        <z>0.0</z>
      </rot>
    </receiver>

    <receiver>
      <port>8002</port>
      <pos>
        <x>0.0</x>
