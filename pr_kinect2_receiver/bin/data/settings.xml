<settings>
  <display>
    <show_floor>1</show_floor>
    <floor_size>12.0</floor_size>
    <show_all_persons>1</show_all_persons>
    <show_reduced_persons>1</show_reduced_persons>
    <joint_radius>0.2</joint_radius>
    <show_target_pos>0</show_target_pos>
    <show_springy_pos>0</show_springy_pos>
    <show_vel>0</show_vel>
    <vel_mult>1.0</vel_mult>
  </display>
  
  <Receivers>
